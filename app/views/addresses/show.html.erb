<div class="page-header">
  <%= link_to addresses_path, :class => 'btn' do %>
    <i class="icon-list-alt"></i>
    Back
  <% end %>
  <%= link_to edit_address_path(@address), :class => 'btn btn-primary' do %>
    <i class="icon-pencil icon-white"></i>
    Edit
  <% end %>
  <h1>Listing addresses</h1>
</div>

<dl>
  <dt>Type:</dt>
  <dd><%= @address.type %></dd>

  <dt>Street:</dt>
  <dd><%= @address.street %></dd>

  <dt>Complement:</dt>
  <dd><%= @address.complement %></dd>

  <dt>City:</dt>
  <dd><%= @address.city %></dd>

  <dt>State:</dt>
  <dd><%= @address.state %></dd>

  <dt>Postal code:</dt>
  <dd><%= @address.postal_code %></dd>

  <dt>Neighbourhood:</dt>
  <dd><%= @address.Neighbourhood %></dd>

</dl>
