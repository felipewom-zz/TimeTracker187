<%= simple_form_for(@version, :defaults => { :input_html => { :class => 'input-xlarge' } }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :item_type %>
    <%= f.input :item_id %>
    <%= f.input :event %>
    <%= f.input :object %>
    <%= f.input :object_changes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "btn-primary" %>
  </div>
<% end %>
